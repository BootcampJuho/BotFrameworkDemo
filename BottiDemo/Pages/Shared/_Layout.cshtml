<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - BottiDemo</title>

    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">

    
    <link rel="stylesheet" href="~/css/site.css" />


    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">


    <link rel="stylesheet" href="~/css/site.min.css" asp-append-version="true" />
    <script src="~/lib/jquery/dist/jquery.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

</head>





<body>

    <div id="tausta">
    </div>

    <!--Navigaatio bar-->

    <nav class="navbar navbar-expand-lg navbar-dark fixed-top" style="background-color: #1a237e;">
        <a class="navbar-brand rotate" href="#"><img src="https://docs.botframework.com/static/devportal/client/images/bot-framework-default.png" width="50" height="50"/></a>


        <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <ul class="navbar-nav mr-auto">

                <li class="nav-item active"><a class="nav-link" asp-page="/Index"><i class="fa fa-fw fa-home"></i>Koti<span class="sr-only">(current)</span></a></li>
                <li class="nav-item"><a class="nav-link" asp-page="/Project"><i class="fa fa-cog fa-spin"></i>Projekti</a></li>
                <li class="nav-item"><a class="nav-link" asp-page="/Tools"><i class="fa fa-cog fa-spin"></i>Tekniikat</a></li>


            </ul>

        </div>
    </nav>

    




    

    <!--Footer-->

    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; 2019 - Bot Framework Demo</p>
        </footer>
    </div>








    <button class="liikkuvaNappi rotate" onclick="ChatNappi()"></button>





    <iframe class="liikkuva" id="Palkki" src='https://webchat.botframework.com/embed/ItseBotti2019?s=K6pqEOWJcYc.eIKbyfjoooOuSHXxPaY0rNyqxgYFx767Zf1KLQcCxcU' style='min-width: 100px; width: 25%; min-height: 300px;'></iframe>




    @RenderSection("Scripts", required: false)
</body>
</html>
